layout: null
permalink: /events.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Your Site//Events//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{% for event in site.events -%}
BEGIN:VEVENT
UID:{{ event.id | absolute_url }}
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ event.start_time | date: "%Y%m%dT%H%M%S" }}
DTEND:{{ event.end_time | date: "%Y%m%dT%H%M%S" }}
SUMMARY:{{ event.title | replace: ',', '\,' | replace: ';', '\;' }}
DESCRIPTION:{{ event.description | strip_html | replace: ',', '\,' replace: ';', '\;' | replace: '\n', '\n' | truncate: 500 }}
URL:{{ event.url | absolute_url }}
END:VEVENT
{% endfor -%}
END:VCALENDAR
